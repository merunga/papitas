Schemas = Schemas or {}
Schemas.Papa = new SimpleSchema
  nombre:
    type: String
    label: "Nombre"
    max: 140

  numero:
    type: Number
    label: "Número"
    unique: true
    min: 1
    max: 90

  audioMarkerIni:
    type: Number
    label: "Audio marker ini"
    decimal: true

  audioMarkerFin:
    type: Number
    label: "Audio marker fin"
    decimal: true

  slug:
    type: String
    label: "Slug"
    max: 140

  imageFileName:
    type: String
    label: "Image file name"
    max: 140


Papas = new Meteor.Collection "papas", {connection: null}
Papas.attachSchema Schemas.Papa

Meteor.startup ->
  for papa in papas
    nroStr = ('00'+papa.numero).slice(-3)
    papa.slug = "#{nroStr}-#{_.slugify papa.nombre}"
    papa.imageFileName = "#{nroStr}-#{papa.nombre}.png"
    Papas.insert papa
